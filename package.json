{
  "name": "annies_havens",
  "version": "1.0.0",
  "type": "module",
  "description": "Annie's Havens Website",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:css && npm run build:js && npm run compile",
    "build:css": "cleancss -o assets/css/styles.min.css assets/css/styles.css",
    "build:js": "terser assets/js/scripts.js -c -m -o assets/js/scripts.min.js",
    "clean": "rimraf dist",
    "compile": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts --fix"
  },
  "author": "Chris Wilson",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "csrf-csrf": "^2.2.2",
    "dotenv": "^17.2.1",
    "express": "^4.17.1",
    "express-handlebars": "^7.1.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "html-minifier-terser": "^7.2.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.23",
    "@types/node": "^20.19.11",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@typescript-eslint/typescript-estree": "^8.39.1",
    "@typescript-eslint/utils": "^8.39.1",
    "clean-css-cli": "^5.6.3",
    "eslint": "^9.33.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "sass": "^1.90.0",
    "terser": "^5.46.0",
    "tsx": "^4.1.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.39.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "extends": "eslint:recommended",
  "rules": {
    "semi": [
      "error",
      "always"
    ]
  }
}
